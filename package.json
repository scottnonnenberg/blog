{
  "name": "blog-scottnonnenberg-com",
  "private": true,
  "version": "1.20.2",
  "scripts": {
    "release": "standard-version",
    "prerelease": "npm run build-production",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write \"*.{css,js,json,md,scss,ts,tsx}\" \"./**/*.{css,js,json,md,scss,ts,tsx}\"",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "build-production": "scripts/build.sh",
    "serve": "cd public && http-server -p 8000 -a localhost",
    "check-internal-links": "broken-link-checker http://localhost:8000/ --recursive --ordered --exclude-external --filter-level=3",
    "check-external-links": "broken-link-checker http://localhost:8000/ --recursive --ordered --exclude-internal --filter-level=3",
    "check-deep-links": "ts-node scripts/check_deep_links.js",
    "check-links": "broken-link-checker --ordered --filter-level=3",
    "generate-rss": "ts-node scripts/generate_rss.js",
    "generate-json": "ts-node scripts/generate_json.js",
    "update-rankings": "ts-node scripts/update_rankings.js",
    "make-post": "ts-node scripts/make_post.js",
    "clean-post": "ts-node scripts/clean_post.js",
    "test": "npm run unit && npm run lint",
    "ci": "npm run unit && npm run lint && npm run check-internal-links && npm run check-deep-links",
    "ci-with-server": "npm-run-all --parallel --race serve ci",
    "mocha": "NODE_ENV=test mocha --recursive --require ts-node/register --require test/setup/index.ts --extension ts",
    "mocha-watch": "npm run mocha -- --watch",
    "unit": "npm run mocha -- -s 3 -t 150 test/unit",
    "unit-watch": "npm run mocha-watch -- -s 3 -t 150 test/unit"
  },
  "dependencies": {
    "gatsby": "2.21.9",
    "gatsby-plugin-less": "3.2.1",
    "gatsby-plugin-no-javascript": "2.0.5",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-typescript": "2.4.2",
    "gatsby-remark-autolink-headers": "2.3.0",
    "gatsby-remark-highlight.js": "0.0.1",
    "gatsby-source-filesystem": "2.3.0",
    "gatsby-transformer-remark": "2.8.4",
    "less": "3.11.1",
    "lodash": "4.17.15",
    "moment": "2.25.2",
    "object-assign": "4.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "underscore.string": "3.3.5"
  },
  "devDependencies": {
    "@scottnonnenberg/eslint-config-thehelp": "0.8.0",
    "@scottnonnenberg/notate": "1.0.0",
    "@types/app-module-path": "2.2.0",
    "@types/async": "^3.2.3",
    "@types/chai": "4.2.11",
    "@types/common-tags": "1.8.0",
    "@types/css-modules-require-hook": "4.0.2",
    "@types/markdown-it": "10.0.1",
    "@types/mocha": "7.0.2",
    "@types/react-helmet": "6.0.0",
    "@types/superagent": "^4.1.7",
    "@types/underscore.string": "0.0.38",
    "app-module-path": "2.2.0",
    "broken-link-checker": "0.7.8",
    "chai": "4.2.0",
    "chalk": "4.0.0",
    "common-tags": "1.8.0",
    "css-modules-require-hook": "4.2.3",
    "feed": "4.1.0",
    "front-matter": "3.1.0",
    "ghooks": "2.0.4",
    "http-server": "0.12.3",
    "markdown-it": "10.0.0",
    "mocha": "7.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "standard-version": "7.1.0",
    "superagent": "5.2.2",
    "ts-node": "8.10.1",
    "typescript": "3.9.2",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "maxSubjectLength": 72
    }
  },
  "author": "Scott Nonnenberg <scott@nonnenberg.com>",
  "license": "MIT",
  "homepage": "https://github.com/scottnonnenberg/blog",
  "repository": {
    "type": "git",
    "url": "git@github.com:scottnonnenberg/blog.git"
  }
}
